{
  "compilerOptions": {
    "declaration": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "noEmitHelpers": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strict": true,
    "target": "es5",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types": [
      "jest"
    ],
    "lib": [
      "dom",
      "es6",
      "es2016.array.include"
    ]
  },
  "exclude": [
    "coverage",
    "dist",
    "docs",
    "node_modules"
  ]
}